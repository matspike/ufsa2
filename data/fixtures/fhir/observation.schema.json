{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Observation",
  "type": "object",
  "properties": {
    "resourceType": {"type": "string", "const": "Observation"},
    "status": {"type": "string", "description": "The status of the result value."},
    "code": {
      "type": "object",
      "description": "Type of observation (code / type)",
      "properties": {
        "coding": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "system": {"type": "string"},
              "code": {"type": "string"},
              "display": {"type": "string"}
            }
          }
        }
      }
    },
    "subject": {"type": "object", "properties": {"reference": {"type": "string"}}},
    "effectiveDateTime": {"type": "string"},
    "valueString": {"type": "string"}
  }
}
