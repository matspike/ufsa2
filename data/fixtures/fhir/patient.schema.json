{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Patient",
  "type": "object",
  "properties": {
    "id": { "type": "string", "description": "Logical id of this artifact" },
    "active": { "type": "boolean" },
    "gender": { "type": "string", "enum": ["male", "female", "other", "unknown"] },
    "name": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "use": { "type": "string" },
          "family": { "type": "string" },
          "given": { "type": "array", "items": { "type": "string" } }
        }
      }
    }
  }
}
